{
  "name": "@neonevm/dao-governance",
  "version": "1.0.0-b2",
  "description": "Neon DAO Governance SDK code base.",
  "files": [
    "./dist/**/*"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "yarn test",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "npm run build",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:lint": "eslint ./dist",
    "test:audit": "npm audit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neonlabsorg/dao-governance-sdk.git"
  },
  "keywords": [
    "neon",
    "dao",
    "governance",
    "solana",
    "web3",
    "library",
    "dapp"
  ],
  "author": "NeonLabs Inc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neonlabsorg/dao-governance-sdk/issues"
  },
  "homepage": "https://github.com/neonlabsorg/dao-governance-sdk#readme",
  "dependencies": {
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "big.js": "^6.2.1",
    "bignumber.js": "^9.1.2",
    "bn.js": "^5.2.1"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/bn.js": "^5.1.5",
    "@types/jest": "^29.5.10",
    "bs58": "^5.0.0",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
